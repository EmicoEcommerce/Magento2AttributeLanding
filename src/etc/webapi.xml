<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="POST" url="/V1/emico-attributelanding/page">
		<service class="Emico\AttributeLanding\Api\LandingPageRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Emico_AttributeLanding::Page_save"/>
		</resources>
	</route>
	<route method="GET" url="/V1/emico-attributelanding/page/search">
		<service class="Emico\AttributeLanding\Api\LandingPageRepositoryInterface" method="getList"/>
		<resources>
			<resource ref="Emico_AttributeLanding::Page_view"/>
		</resources>
	</route>
	<route method="GET" url="/V1/emico-attributelanding/page/:pageId">
		<service class="Emico\AttributeLanding\Api\LandingPageRepositoryInterface" method="getById"/>
		<resources>
			<resource ref="Emico_AttributeLanding::Page_view"/>
		</resources>
	</route>
	<route method="PUT" url="/V1/emico-attributelanding/page/:pageId">
		<service class="Emico\AttributeLanding\Api\LandingPageRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Emico_AttributeLanding::Page_update"/>
		</resources>
	</route>
	<route method="DELETE" url="/V1/emico-attributelanding/page/:pageId">
		<service class="Emico\AttributeLanding\Api\LandingPageRepositoryInterface" method="deleteById"/>
		<resources>
			<resource ref="Emico_AttributeLanding::Page_delete"/>
		</resources>
	</route>
</routes>
